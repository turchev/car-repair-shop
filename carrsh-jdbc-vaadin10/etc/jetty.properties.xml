<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<!-- Jetty configuration -->
<properties>
    <comment>Jetty server configuration</comment>    
    <!-- the configured port for the connector or 0 a random available port may be used -->
    <entry key="PORT">8080</entry>
    <entry key="HOST">localhost</entry>	
    <entry key="IDLE_TIME_OUT">30000</entry>
    <!-- default maximum size Jetty permits is 200000 bytes and 1000 keys -->
    <entry key="MAX_FORM_CONTENT_SIZE">1000</entry>
    <entry key="MAX_FORM_KEYS">10</entry>
</properties>
